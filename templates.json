[
  {
    "path": ".github/**",
    "handlers": [
      {
        "name": "ignore",
        "options": {
          "eval": "resource.repository.isGitHubRepo === false"
        }
      }
    ]
  },
  {
    "path": "package.json.hbs",
    "handlers": [
      "handlebars",
      "merge-json"
    ]
  }
]
